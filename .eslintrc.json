{
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true
  },
  "globals": {
    "afterEach": true,
    "beforeEach": true,
    "CONFIG": true,
    "describe": true,
    "expect": true,
    "global": true,
    "it": true,
    "jasmine": true,
    "jest": true,
    "module": true,
    "require": true,
    "spyOn": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },
  "rules": {
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-class-assign": 1,
    "no-cond-assign": 2,
    "no-console": 1,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-continue": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 2,
    "no-dupe-keys": 2,
    "no-dupe-args": 2,
    "no-duplicate-case": 2,
    "no-else-return": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 1,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": [2, "functions"],
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implicit-coercion": 2,
    "no-implied-eval": 2,
    "no-inline-comments": 1,
    "no-inner-declarations": [2, "functions"],
    "no-invalid-regexp": 2,
    "no-invalid-this": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-mixed-requires": [2, false],
    "no-mixed-spaces-and-tabs": [2, false],
    "linebreak-style": [2, "unix"],
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": [1, {"max": 1}],
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-nested-ternary": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 2,
    "no-path-concat": 2,
    "no-plusplus": [2, {"allowForLoopAfterthoughts": true}],
    "no-process-env": 0,
    "no-process-exit": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-restricted-modules": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-sync": 0,
    "no-ternary": 0,
    "no-trailing-spaces": 2,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-unexpected-multiline": 2,
    "no-underscore-dangle": [1, { "allowAfterThis": true }],
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": [2, {"vars": "local", "args": "after-used"}],
    "no-use-before-define": 2,
    "no-useless-call": 2,
    "no-void": 2,
    "no-var": 1,
    "no-warning-comments": [1, { "terms": ["todo", "fixme", "xxx"], "location": "start" }],
    "no-with": 1,
    "array-bracket-spacing": [1, "never"],
    "arrow-parens": [2, "as-needed"],
    "arrow-spacing": 1,
    "accessor-pairs": 1,
    "block-scoped-var": 1,
    "brace-style": [1, "1tbs"],
    "callback-return": 1,
    "camelcase": 0,
    "comma-dangle": [2, "never"],
    "comma-spacing": 2,
    "comma-style": 2,
    "complexity": [1, 11],
    "computed-property-spacing": [0, "never"],
    "consistent-return": 0,
    "consistent-this": [0, "that"],
    "constructor-super": 0,
    "curly": [0, "all"],
    "default-case": 0,
    "dot-location": 0,
    "dot-notation": [0, { "allowKeywords": true }],
    "eol-last": 1,
    "eqeqeq": 1,
    "func-names": 0,
    "func-style": [0, "declaration"],
    "generator-star-spacing": 0,
    "guard-for-in": 0,
    "handle-callback-err": 0,
    "id-length": 0,
    "id-match": 0,
    "indent": ["error", 2, {"VariableDeclarator": { "var": 2, "let": 2, "const": 3 }, "SwitchCase": 1}],
    "init-declarations": 0,
    "jsx-quotes": 1,
    "key-spacing": 2,
    "keyword-spacing": 2,
    "lines-around-comment": 0,
    "max-depth": [0, 4],
    "max-len": [0, 80, 4],
    "max-nested-callbacks": [0, 2],
    "max-params": [0, 3],
    "max-statements": [0, 10],
    "new-cap": 0,
    "new-parens": 0,
    "newline-after-var": 0,
    "object-curly-spacing": [2, "always"],
    "object-shorthand": 0,
    "one-var": 0,
    "operator-assignment": [0, "always"],
    "operator-linebreak": 1,
    "padded-blocks": 0,
    "prefer-const": 2,
    "prefer-spread": 2,
    "prefer-reflect": 0,
    "quote-props": 0,
    "quotes": [2, "single"],
    "radix": 0,
    "react/display-name": 0,
    "react/jsx-boolean-value": 0,
    "react/jsx-curly-spacing": 1,
    "react/jsx-max-props-per-line": 0,
    "react/jsx-no-duplicate-props": 1,
    "react/jsx-no-undef": 1,
    "react/sort-prop-types": 1,
    "react/jsx-sort-props": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/no-danger": 1,
    "react/no-did-mount-set-state": 1,
    "react/no-did-update-set-state": 1,
    "react/no-multi-comp": [1, { "ignoreStateless": true }],
    "react/no-unknown-property": 1,
    "react/prop-types": [1, {"ignore": ["content", "copy", "title"]}],
    "react/react-in-jsx-scope": 1,
    "react/require-extension": 0,
    "react/self-closing-comp": 1,
    "react/sort-comp": 1,
    "require-yield": 0,
    "semi": 2,
    "semi-spacing": 2,
    "sort-vars": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": 2,
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": 2,
    "strict": 2,
    "use-isnan": 2,
    "valid-jsdoc": 2,
    "valid-typeof": 2,
    "vars-on-top": 2,
    "wrap-iife": 1,
    "wrap-regex": 2,
    "yoda": 2
  }
}
